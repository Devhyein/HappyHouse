<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="com.ssafy.happyhouse.model.dao.HouseInfoDao">
	
	<select id="searchAllHouseInfo">
		select * from houseInfo where 1=1;
	</select>
	
	<insert id="insertHouseInfo">
		insert into 
		houseInfohouseInfo(dong, aptName, code, buildYear, jibun, lat, lng, img)
		values(#{dong},#{AptName},#{code},#{buildYear},#{jibun},#{lat},#{lng},#{img})";
	</insert>

<!-- 	<update id="updateHouseInfo">
	
	</update>
	
	<delete id="deleteHouseInfo">
	
	</delete> -->
</mapper>